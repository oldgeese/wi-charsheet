{
  "name": "wi-charsheet",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/wi-charsheet/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/wi-charsheet",
        "index": "apps/wi-charsheet/src/index.html",
        "baseHref": "/",
        "main": "apps/wi-charsheet/src/main.tsx",
        "polyfills": "apps/wi-charsheet/src/polyfills.ts",
        "tsConfig": "apps/wi-charsheet/tsconfig.app.json",
        "assets": [
          "apps/wi-charsheet/src/favicon.svg",
          "apps/wi-charsheet/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nx/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/wi-charsheet/src/environments/environment.ts",
              "with": "apps/wi-charsheet/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/web:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "wi-charsheet:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "wi-charsheet:build:development"
        },
        "production": {
          "buildTarget": "wi-charsheet:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/wi-charsheet/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/wi-charsheet"],
      "options": {
        "jestConfig": "apps/wi-charsheet/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/wi-charsheet/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "configDir": "apps/wi-charsheet/.storybook",
        "outputDir": "dist/storybook/wi-charsheet"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
